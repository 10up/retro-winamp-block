{
	"apiVersion": 2,
	"name": "tenup/webamp-block",
	"version": "0.1.0",
	"title": "Webamp Block",
	"category": "media",
	"icon": "playlist-audio",
	"description": "A Winamp-styled audio block for all your retro music player needs.",
	"attributes": {
		"audio": {
			"type": "array",
			"default": [],
			"source": "query",
			"selector": ".blocks-audio-grid",
			"query": {
				"url": {
					"type": "string",
					"source": "attribute",
					"selector": "audio",
					"attribute": "src"
				}
			}
		},
		"ids": {
			"type": "array",
			"items": {
				"type": "number"
			},
			"default": []
		},
		"currentSkin": {
			"type": "string",
			"default": ""
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "webamp-block",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"script": "webamp-block-frontend",
	"style": "file:./build/style-index.css"
}
